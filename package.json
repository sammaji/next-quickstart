{
  "name": "next-starter",
  "version": "1.4.1",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev -p 3000",
    "dev:tr": "cross-env NODE_ENV=development next dev --turbo -p 3000",
    "dev:pr": "prisma generate && cross-env NODE_ENV=development next dev",
    "build": "npm run prettier:fix && cross-env NODE_ENV=production next build",
    "start": "cross-env NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db": "prisma migrate dev && prisma generate",
    "stripe:listen": "stripe listen --events checkout.session.completed --forward-to localhost:3000/api/webhook/checkout",
    "stripe:trigger": "stripe trigger checkout.session.completed",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.2",
    "@clerk/nextjs": "^4.25.1",
    "@prisma/client": "^5.3.1",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/node": "20.8.2",
    "@types/react": "18.2.24",
    "@types/react-dom": "18.2.14",
    "autoprefixer": "10.4.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "eslint": "8.50.0",
    "eslint-config-next": "13.4.19",
    "next": "13.5.4",
    "next-themes": "^0.2.1",
    "postcss": "8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.11.0",
    "sharp": "^0.32.6",
    "stripe": "^13.8.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-interactions": "^7.6.12",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.5.2",
    "@storybook/nextjs": "^7.4.6",
    "@storybook/react": "^7.4.6",
    "@storybook/testing-library": "^0.2.2",
    "cross-env": "^7.0.3",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.0",
    "prisma": "^5.3.1",
    "storybook": "^7.5.3"
  }
}
